{
  "name": "array-json",
  "version": "0.2.1",
  "description": "Utility Functions for handle Array as JSON files",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./file-system": {
      "types": "./dist/file-system/index.d.ts",
      "import": "./dist/file-system/index.js",
      "require": "./dist/file-system/index.cjs"
    },
    "./array-object": {
      "types": "./dist/array-object/index.d.ts",
      "import": "./dist/array-object/index.js",
      "require": "./dist/array-object/index.cjs"
    },
    "./crypto": {
      "types": "./dist/crypto/index.d.ts",
      "import": "./dist/crypto/index.js",
      "require": "./dist/crypto/index.cjs"
    },
    "./zlib": {
      "types": "./dist/zlib/index.d.ts",
      "import": "./dist/zlib/index.js",
      "require": "./dist/zlib/index.cjs"
    },
    "./util": {
      "types": "./dist/util/index.d.ts",
      "import": "./dist/util/index.js",
      "require": "./dist/util/index.cjs"
    }
  },
  "scripts": {
    "test": "jest",
    "build": "tsup"
  },
  "keywords": [
    "JSON",
    "Array",
    "readJson",
    "writeJson"
  ],
  "author": "Pho Thin Maung",
  "homepage": "https://phothinmg.github.io/array-json/",
  "license": "Apache-2.0",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typedoc": "^0.25.8",
    "typedoc-google-tag-manager-plugin": "^1.0.5",
    "typedoc-plugin-clarity": "^1.6.0",
    "typedoc-plugin-extras": "^3.0.0",
    "typedoc-plugin-include-example": "^1.2.0",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phothinmg/array-json.git"
  }
}
